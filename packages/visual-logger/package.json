{
  "name": "visual-logger",
  "version": "1.1.3",
  "description": "Visual logger for CLI in NodeJS",
  "type": "module",
  "types": "dist-cjs/index.d.ts",
  "main": "dist-cjs/index.cjs",
  "module": "dist-esm/index.js",
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist-cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rm -rf dist-* && tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json && ts2mjs --cjs --skip-ts --remove-source dist-cjs",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "xrun build",
    "prepack": "publish-util-prepack",
    "postpack": "publish-util-postpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/visual-logger.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "visual",
    "cli",
    "logger",
    "log",
    "logging",
    "console",
    "terminal",
    "term",
    "update",
    "progress",
    "bar",
    "stdout",
    "tty",
    "spinner",
    "animation",
    "refresh"
  ],
  "files": [
    "dist-cjs",
    "dist-esm"
  ],
  "author": "Joel Chen <joel123@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^4.0.0",
    "log-update": "^4.0.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@vitest/coverage-v8": "^3.2.2",
    "prettier": "^3.5.3",
    "publish-util": "^2.1.0",
    "ts2mjs": "^4.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  },
  "prettier": {
    "printWidth": 100
  }
}
