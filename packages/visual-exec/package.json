{
  "name": "visual-exec",
  "version": "0.2.0",
  "description": "Visual shell exec with spinners and progress display",
  "type": "module",
  "types": "dist-cjs/index.d.ts",
  "main": "dist-cjs/index.cjs",
  "module": "dist-esm/index.js",
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist-cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rm -rf dist-* && tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json && ts2mjs --cjs --skip-ts --remove-source dist-cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "xrun --serial build test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jchip/fynjs.git",
    "directory": "packages/visual-exec"
  },
  "keywords": [
    "visual",
    "shell",
    "exec",
    "spinner",
    "progress"
  ],
  "files": [
    "dist-cjs",
    "dist-esm"
  ],
  "author": "Joel Chen <joel123@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^4.1.2",
    "ci-info": "^3.2.0",
    "has-ansi": "^4.0.1",
    "strip-ansi": "^6.0.0",
    "visual-logger": "^1.1.3",
    "xsh": "^0.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.2",
    "ts2mjs": "^4.0.0",
    "tsx": "^4.20.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
