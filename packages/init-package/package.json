{
  "name": "init-package",
  "version": "1.0.0",
  "description": "initialize package.json for npm package",
  "type": "module",
  "types": "dist-cjs/index.d.ts",
  "main": "dist-cjs/index.cjs",
  "module": "dist-esm/index.js",
  "exports": {
    ".": {
      "import": "./dist-esm/index.js",
      "require": "./dist-cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=20"
  },
  "homepage": "",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rm -rf dist-* && tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json && ts2mjs --cjs --skip-ts --remove-source dist-cjs",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "private": true,
  "author": "",
  "files": [
    "dist-cjs",
    "dist-esm"
  ],
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "semver": "^7.3.5",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.15.30",
    "@types/semver": "^7.5.8",
    "@vitest/coverage-v8": "^3.2.2",
    "enquirer": "^2.3.6",
    "prettier": "^3.5.3",
    "ts2mjs": "^4.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  },
  "prettier": {
    "printWidth": 100
  }
}
